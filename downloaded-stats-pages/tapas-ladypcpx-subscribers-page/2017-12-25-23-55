<script type="text/javascript">

  function videosAvailable (res) {
    panda.require('coins.hyprmx').toggleVideoBtn(res);
  }

  window.addEventListener("message", hyprMxAdComplete, false);

  function hyprMxAdComplete(event) {
    var origin = panda.getEnvs('appDomain');
    if(panda.getEnvs('isMobileView')) {
      origin = 'https://m.tapas.io'
    }

    if (~event.origin.indexOf(origin) && event.data === 'hyprMxCompleted' ) {
      setTimeout(function () {
        panda.require('coins.hyprmx').reward(event);
      }, 1000);
    }
  }
</script>
