<script type="text/javascript">
$(function() {
	function setInfoLayoutScrollbox() {
		var patronScrollBox;
		
		$("#seeDetail").on("click", function(e){
			e.preventDefault();
			
			$("#ly_patron_guide_dim").addClass("on");
			$("#ly_patron_guide").addClass("on");

			$("body").css('position', 'fixed');
			if(!patronScrollBox) {
				patronScrollBox = new IScroll('#patron_scrollbox .scrollbar-box', {
					useTransform : true,
					resizeScrollbars : true,
					scrollX : false,
					scrollY : true,
					scrollbars: 'custom',
					mouseWheel: true,
					interactiveScrollbars: true,
			        disableMouse: true
				});
			}
		});

		$('#ly_patron_guide').on('click', '.btn_close', function(e) {
			e.preventDefault();
			
			$("#ly_patron_guide_dim").removeClass('on');
			$("#ly_patron_guide").removeClass('on');
			$("body").css('position', 'relative');
			return false;
		});
	}
	function signExpectedWebtoonBonusArea() {
		var episodeCountEligible = true;
		var subscriberBonusEligible = true;
		var pvsBonusEligible = true;
		
		var activeEpisodeCountAreaId = "#_episode_";
		var activeSubscriberBonusAreaId = "#_subscriber_";
		var activePvsBonusAreaId = "#_pvs_";
		
		if(episodeCountEligible) {
			activeEpisodeCountAreaId += "eligible";
		} else {
			activeEpisodeCountAreaId += "not_eligible";
		}
		
		if(subscriberBonusEligible) {
			activeSubscriberBonusAreaId += 400;
		} else {
			activeSubscriberBonusAreaId += "not_eligible";
		}
		
		if(pvsBonusEligible) {
			activePvsBonusAreaId += 500;
		} else {
			activePvsBonusAreaId += "not_eligible";
		}
		
		$(activeEpisodeCountAreaId).addClass("on");
		$(activeSubscriberBonusAreaId).addClass("on").prev().addClass("on");
		$(activePvsBonusAreaId).addClass("on").prev().addClass("on");
	}
	
	function setCheerUpText() {
		var showLastMonth = true;
		var isRegisteredPatreonUser = true;
		var seeDetail = $("#seeDetail");
		
		var doRegisterText = "Enter your Patreon ID and let us become your Patron";
		var cheerUpText = "Keep it up and let us become your Patron";
		if(showLastMonth) {
			if(!isRegisteredPatreonUser) {
				seeDetail.prepend(doRegisterText);
			} else {
				$.ajax({
					url : '/challenge/author/getPatreonInfo',
					data : {
						userId : '2332042'
					}
				}).then($.proxy(function(res) {
					if (res) {
						if(res.webtoonBonus) {
							seeDetail.prepend("Congratulations! Webtoon has pledged $" + Util.changeNumberFormat(res.webtoonBonus) + " to you as your Patron!");
						} else {
							seeDetail.prepend(cheerUpText);
						}
					}
				}, this));
			}			
		} else {
			if(!isRegisteredPatreonUser) {
				seeDetail.prepend(doRegisterText);
			} else {
				seeDetail.prepend(cheerUpText);
			}
		}
	}
	
	setInfoLayoutScrollbox();
	signExpectedWebtoonBonusArea();
	setCheerUpText();

	var titleStat = new TitleStat();
});
</script>


