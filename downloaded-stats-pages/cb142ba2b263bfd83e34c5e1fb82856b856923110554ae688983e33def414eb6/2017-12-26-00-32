<script type="text/javascript" src="./2017-12-26-00-32_files/ranking.js" charset="UTF-8"></script>
<script type="text/javascript">
	window.genreNameMap = {
		
		'DRAMA' : 'Drama',
		
		'FANTASY' : 'Fantasy',
		
		'COMEDY' : 'Comedy',
		
		'ACTION' : 'Action',
		
		'SLICE_OF_LIFE' : 'Slice of life',
		
		'ROMANCE' : 'Romance',
		
		'SUPER_HERO' : 'Superhero',
		
		'HEARTWARMING' : 'Heartwarming',
		
		'HISTORICAL' : 'Historical',
		
		'THRILLER' : 'Thriller',
		
		'SPORTS' : 'Sports',
		
		'SF' : 'Sci-fi',
		
		'HORROR' : 'Horror',
		
		'TIPTOON' : 'Informative'
		
	};

	$(window).on('load', function() {
		var genreRanking = new Ranking({
			layerId : '#genreRanking',
			templateId : '#tplRankingTitle',
			genreNameMap : window.genreNameMap,
			apiInfo : {
				filterParamName : 'rankingGenre',
				url : '/en/ranking/getGenreRanking',
				count : 3
			}
		});
		var ageRanking = new Ranking({
			layerId : '#ageRanking',
			templateId : '#tplRankingTitle',
			genreNameMap : window.genreNameMap,
			apiInfo : {
				filterParamName : 'target',
				url : '/en/ranking/getAgeRanking',
				count : 3
			}
		});

		// 랭킹 타이틀 링크 클릭 이벤트 처리(현재 선택된 랭킹 필터로 랭킹 페이지 이동)
		$("._rankingMoveBtn").on('click', function(e){
			e.preventDefault();
			var sGenreRankingFilter = genreRanking.getSelectedFilter();
			var sAgeRankingFilter = ageRanking.getSelectedFilter();
			location.href = '/' + contentLanguageCode + '/top?rankingGenre=' + sGenreRankingFilter + '&target=' + sAgeRankingFilter;
		});
	});
</script>

