<script type="text/javascript">
function goPrivacy() {
	var currentLocation = window.location.href;
	var termsAndPrivacyUrl = "http://www.webtoons.com/en/terms";
	
	if(currentLocation.match(termsAndPrivacyUrl)) {
		window.location.reload(true);
	}
}
</script>

	
	
	



