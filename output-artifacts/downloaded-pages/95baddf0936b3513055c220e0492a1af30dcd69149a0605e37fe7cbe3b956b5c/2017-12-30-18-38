	<script type="text/javascript" src="/compress/release/js/episodeList_20171228-1130.js" charset="utf-8"></script>
	<script type="text/javascript">
	// 토너먼트 공모전 투표하기 관련 메시지
	
	
	var addFavorite;
	var gsLanguage = 'en';
	$(function() {
		new EpisodeList({
			titleNo: 81223,
			btnEpisodeText: "Continue reading",
			language: "en",
			isChallenge : true,
			episodePath: "http://www.webtoons.com/en/challenge/camellia",
			messages : {
				registerError : "System error!",
			}
			
			, cheer : new Cheer({
				titleNo : 81223,
				minimumMessage : "",
				cheerMessage : "",
				cheerCompleteMessage : "",
				languageCode : 'en',
				nclkLanguageCode : 'en_en'
			})		
		});
	});

	$(window).on('load', function() {
		// 공유하기
		var share = new Share({
			shareContentFunction: getShareContent,
			shareCopyMessage: "The URL has been copied.<br>Paste(Ctrl+V) it in the desired location.",
			clipboardSwf: "/swf/clipboard.swf",
			serviceZone: "GLOBAL",
			titleNo : 81223,
			isChallenge : true,
			fbRefresh : "1513832495000"
		});
		// 관심 등록
		addFavorite = new AddFavorite({
			titleNo: 81223,
			isChallenge : true,
			onFavoriteButtonChanged : share._setClipboardPosition,
			areaCode : "enc",
			favoriteButtonHTML : '<em class="ico_plus4"></em>Subscribe',
			alreadyFavoritedButtonHTML : '<em class="ico_plus4"></em>Subscribed',
			promotionName : ''
		});
		// 좋아요 개수
		new LikeItForList({
			language : gsLanguage,
			isChallenge : true,
			titleNo : 81223
		});
		
		Util.challengeRewardLog({
			type : 'CHALLENGE_ACCESS',
			language : gsLanguage
		});
		
		// 챌린지리그 리워드 status bar
		new ChallengeRewardStatusBar({areaCode : 'enc'});
		
		
	});
	
	function getShareContent(sns) {
		var title = "Camellia";
		var titleSynopsis = "In ancient fantasy China, a clumsy villager gets more than he bargained for when a strange boy follows him home. A cool comic about sexy boys in the woods gettin&#39; it on!";
		
		
			
			var episodeTitle = "";
		
		
		
		var shareUrl = location.href;
		
		function replacement(shareMessage, title, episodeTitle, titleSynopsis, shareUrl) {
			shareMessage = shareMessage.replace("{TITLE_NAME}", title).replace("{EPISODE_NAME}", episodeTitle).replace("{LINK_URL}", shareUrl).replace("{TITLE_SYNOPSIS}", titleSynopsis);
			shareMessage = shareMessage.replace("<br>", "\n").replace("&#39;", "'").replace('&quot;', '"').replace('&lt;', '<').replace('&gt;', '>').replace('&amp;', '&');
			return shareMessage;
		}
		switch(sns) {
		case 'WEIBO' :
			var shareMessage = '{TITLE_NAME} 来来来我安利你一部超好看的漫画@WEBTOON每日漫画\n';
			shareMessage = replacement(shareMessage, title, episodeTitle, titleSynopsis, shareUrl);
			return {shareMessage : encodeURIComponent(shareMessage)};
			break;
		case 'TWITTER' :
			var shareMessage = 'Reading #{TITLE_NAME} made my day better.\n {LINK_URL} #LINEWEBTOON';
			title = title.replace(/\s/g, "_");
			shareMessage = replacement(shareMessage, title, episodeTitle, titleSynopsis, shareUrl);
			return encodeURIComponent(shareMessage);
			break;
		case 'TUMBLR_NAME':
			var shareTitle = '{TITLE_NAME}';
			shareTitle = replacement(shareTitle, title, episodeTitle, titleSynopsis, shareUrl);
			return encodeURIComponent(shareTitle);	
			break;
		case 'REDDIT':
			var shareTitle = '{TITLE_NAME} This comic will make your day better!';
			shareTitle = replacement(shareTitle, title, episodeTitle, titleSynopsis, shareUrl);
			return encodeURIComponent(shareTitle);
			break;
		case 'TUMBLR_DESCRIPTION' :
			var shareDescription = '{TITLE_SYNOPSIS}\nReading this will make your day better. #{TITLE_NAME} #webcomic #LINEWEBTOON';
			shareDescription = replacement(shareDescription, title, episodeTitle, titleSynopsis, shareUrl);
			return encodeURIComponent(shareDescription);
			break;
		case 'LINE' :
			var shareMessage = 'You have to check out {TITLE_NAME} on LINE WEBTOON!';
			shareMessage = replacement(shareMessage, title, episodeTitle, titleSynopsis, shareUrl);
			return encodeURIComponent(shareMessage);
			break;
		case 'QQ':
			var shareMessage = '';
			shareMessage = replacement(shareMessage, title, episodeTitle, titleSynopsis, shareUrl);
			return {shareMessage : encodeURIComponent(shareMessage)};
			break;
		}
		
		return encodeURIComponent(title + " " + location.href + " by ");
	}
	
	
	$(window).on('load', function() {
		new Patreon().loadPatronageInfo(2332042);
	});
	
	if ("Camellia".length > 30) { 
		$('._challengeTitle').addClass('long_text');
	}
	</script>
</div>

